<html>
<head>
  <title>Log In</title>
  <h1>Please Sign In</h1>

  <%= form_tag '/users/sign_in', method: :post do %>
      <p>Email</p>
      <input type="text" placeholder="email..." name="email">
      <p>Password</p>
      <input type="text" placeholder="pass..." name="password">

      <button>Create Issue</button>
  <% end %>
</head>
</html>